{
  "name": "ToDoList",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && npm i",
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm run lint -s && npm test -s"
  },
  "dependencies": {
    "apisauce": "^0.10.0",
    "format-json": "^1.0.3",
    "lodash": "^4.17.2",
    "querystringify": "0.0.4",
    "ramda": "^0.23.0",
    "react": "15.4.1",
    "react-native": "0.42.0",
    "react-native-animatable": "^1.2.0",
    "react-native-checkbox": "^1.1.0",
    "react-native-config": "^0.2.1",
    "react-native-device-info": "^0.10.0",
    "react-native-swipeout": "^2.1.1",
    "react-native-vector-icons": "^4.1.1",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.14.3",
    "reduxsauce": "0.4.1",
    "seamless-immutable": "^7.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "20.0.3",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "1.9.2",
    "enzyme": "^2.8.2",
    "husky": "^0.13.1",
    "jest": "20.0.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "snazzy": "^6.0.0",
    "standard": "8.6.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "expect",
      "it",
      "beforeEach",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
